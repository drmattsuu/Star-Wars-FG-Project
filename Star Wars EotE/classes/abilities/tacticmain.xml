<?xml version="1.0" encoding="iso-8859-1"?>
<root>

	<windowclass name="tacticmain">
		<placement>
			<size>
				<width>300</width>
				<height>334</height>
			</size>
		</placement>
		<nodelete />
		<sheetdata>
		
<!-- NAME -->
			<sheetframe name="main_name">
				<bounds>12,20,264,40</bounds>
			</sheetframe>
			
			<windowshortcut name="main_name_shortcut">
				<anchored>
					<to>main_name</to>
					<position>insidetopleft</position>					
					<offset>11,11</offset>
					<size>
						<height>20</height>
						<width>20</width>
					</size>
				</anchored>			
				<class>tactic</class>
			</windowshortcut>
			<basestringfield name="main_name_name" source="name">
				<anchored>			
					<to>main_name</to>
					<position>insidetopleft</position>					
					<offset>32,14</offset>
					<size>
						<height>20</height>
						<width>200</width>
					</size>
				</anchored>
				<empty>&#171; New Tactic &#187;</empty>
			</basestringfield>		
			
<!-- DESCRIPTION -->
			<sheetframe name="main_description">
				<bounds>12,60,264,260</bounds>
			</sheetframe>
			<sheetframetitle name="main_description_title">
				<anchored>
					<to>main_description</to>
				</anchored>
				<static>Description</static>
			</sheetframetitle>
			
			<sheetformattedtextfield name="main_description_description" source="description">
				<anchored>
					<left>
						<parent>main_description</parent>
						<anchor>left</anchor>
						<offset>13</offset>					
					</left>
					<right>
						<parent>main_description</parent>
						<anchor>right</anchor>
						<offset>-13</offset>					
					</right>
					<top>
						<parent>main_description</parent>
						<anchor>top</anchor>
						<offset>18</offset>
					</top>
					<bottom>
						<parent>main_description</parent>
						<anchor>bottom</anchor>
						<offset>-13</offset>
					</bottom>
				</anchored>
			</sheetformattedtextfield>
			<scrollbar>
				<anchored>
					<to>main_description_description</to>
					<offset>-3,-2</offset>
				</anchored>
				<target>main_description_description</target>
			</scrollbar>			
			
			<closebutton />
		</sheetdata>
	</windowclass>
	
</root>