						<parent>notes_notes</parent>
						<anchor>top</anchor>
						<offset>18</offset>
					</top>
					<bottom>
						<parent>notes_notes</parent>
						<anchor>bottom</anchor>
						<offset>-13</offset>
					</bottom>
				</anchored>		
			</sheetformattedtextfield>
			<scrollbar>
				<anchored>
					<to>notes_notes_notes</to>
					<offset>-3,-2</offset>
				</anchored>
				<target>notes_notes_notes</target>
			</scrollbar>
			
		</sheetdata>
