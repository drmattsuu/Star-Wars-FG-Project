					<position>insidetopleft</position>					
					<offset>32,14</offset>
					<size>
						<height>20</height>
						<width>200</width>
					</size>
				</anchored>
				<empty>&#171; New NPC Group &#187;</empty>
				<tabtarget>
					<next>notes_name_name</next>
					<prev>notes_name_name</prev>
				</tabtarget>					
			</basestringfield>
			
<!-- NOTES -->
			<sheetframe name="notes_notes">
				<bounds>12,60,472,388</bounds>
			</sheetframe>
			<sheetframetitle name="notes_notes_title">
