				<script>
					function onInit()
						registerMenuItem("Create Item", "insert", 5);
					end
					
					function onMenuSelection(selection)
						window.list.createWindow();
					end
				</script>
			</genericcontrol>		
			<basestringcontrol>
				<static>Species and Special Abilities</static>			
