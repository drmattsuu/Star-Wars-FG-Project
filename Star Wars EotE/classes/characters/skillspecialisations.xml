<?xml version="1.0" encoding="iso-8859-1"?>
<root>

	<windowclass name="skillspecialisations">
		<placement>
			<size>
				<width>300</width>
				<height>334</height>
			</size>
		</placement>
		<nodelete />
		<sheetdata>
		
<!-- NAME -->
			<sheetframe name="specialisations_name">
				<bounds>12,20,264,40</bounds>
			</sheetframe>
			
			<windowshortcut name="specialisations_name_shortcut">
				<anchored>
					<to>specialisations_name</to>
					<position>insidetopleft</position>					
					<offset>11,11</offset>
					<size>
						<height>20</height>
						<width>20</width>
					</size>
				</anchored>			
				<class>skill</class>
			</windowshortcut>
			<basestringfield name="specialisations_name_name" source="name">
				<anchored>			
					<to>specialisations_name</to>
					<position>insidetopleft</position>					
					<offset>32,14</offset>
					<size>
						<height>20</height>
						<width>200</width>
					</size>
				</anchored>
				<empty>&#171; New Skill &#187;</empty>
				<tabtarget>
					<next>specialisations_value</next>
					<prev>specialisations_encumbrance</prev>
				</tabtarget>				
			</basestringfield>
			<skilldicepool>
				<anchored>			
					<to>specialisations_name</to>
					<position>insidetopright</position>					
					<offset>9,11</offset>
					<size>
						<height>20</height>
						<width>20</width>
					</size>
				</anchored>
			</skilldicepool>			
			
<!-- SPECIALISATIONS -->
			<sheetframe name="specialisations_specialisations">
				<bounds>12,60,264,260</bounds>
			</sheetframe>
			<sheetframetitle name="specialisations_specialisations_title">
				<anchored>
					<to>specialisations_specialisations</to>
				</anchored>
				<static>Specialisations</static>
			</sheetframetitle>

			<-- SPECIALISATIONS -->			
			<sheetlist name="specialisations_specialisations_specialisations">
				<anchored>
					<left>
						<parent>specialisations_specialisations</parent>
						<anchor>left</anchor>
						<offset>12</offset>					
					</left>
					<right>
						<parent>specialisations_specialisations</parent>
						<anchor>right</anchor>
						<offset>-13</offset>					
					</right>
					<top>
						<parent>specialisations_specialisations</parent>
						<anchor>top</anchor>
						<offset>18</offset>
					</top>
					<bottom>
						<parent>specialisations_specialisations</parent>
						<anchor>bottom</anchor>
						<offset>-13</offset>
					</bottom>
				</anchored>
				<datasource>.specialisations</datasource>
				<class>specialisationsmall</class>
				<allowcreate />
				<acceptdrop>
					<class>specialisation</class>
					<field>*</field>
				</acceptdrop>
			</sheetlist>
			<scrollbar>
				<anchored>
					<to>specialisations_specialisations_specialisations</to>
					<offset>-3,-2</offset>
				</anchored>
				<target>specialisations_specialisations_specialisations</target>
			</scrollbar>			

		</sheetdata>
	</windowclass>
	
</root>